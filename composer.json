{
    "name": "simonebianco/laravel-dolphin-parser",
    "description": "Laravel client for Dolphin PDF Parser - Extract text, tables, and figures from PDFs using AI",
    "keywords": ["laravel", "pdf", "parser", "dolphin", "ocr", "document", "ai"],
    "license": "MIT",
    "authors": [
        {
            "name": "Simone Bianco",
            "email": "simone@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "SimoneBianco\\DolphinParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimoneBianco\\DolphinParser\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimoneBianco\\DolphinParser\\DolphinParserServiceProvider"
            ],
            "aliases": {
                "DolphinParser": "SimoneBianco\\DolphinParser\\Facades\\DolphinParser"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
